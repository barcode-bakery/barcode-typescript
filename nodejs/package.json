    "@types/node": "^20",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "eslint": "^9.1.1",
    "eslint-config-next": "14.2.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.1",
    "globals": "^15.0.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.7",
    "typescript": "^5"
  }
