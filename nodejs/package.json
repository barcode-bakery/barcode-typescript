{
  "name": "@barcode-bakery/barcode-nodejs",
  "version": "1.0.0",
  "description": "Base code for generating barcode with the Barcode Bakery library with Node. See barcode-bakery-1d.",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/barcode-bakery/barcode-typescript",
    "directory": "nodejs/common"
  },
  "homepage": "https://www.barcodebakery.com",
  "author": {
    "name": "Jean-SÃ©bastien Goupil",
    "email": "contact@barcodebakery.com"
  },
  "scripts": {
    "prepublishOnly": "npm run clean && tsc",
    "build": "npm run clean && tsc",
    "lint": "eslint",
    "clean": "rimraf ./dist"
