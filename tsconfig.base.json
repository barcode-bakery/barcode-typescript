{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["es2020"],
    "declaration": true,
    "useDefineForClassFields": true,
    "allowJs": true,
    "skipLibCheck": true,
    "allowImportingTsExtensions": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "es2020",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "incremental": true
  }
}
